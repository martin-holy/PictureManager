<ResourceDictionary
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:mhuInt="clr-namespace:MH.Utils.Interfaces;assembly=MH.Utils"
  xmlns:utils="clr-namespace:MH.UI.WPF.Utils">

  <ResourceDictionary.MergedDictionaries>
    <utils:CachedResourceDictionary Source="../Styles/Border.xaml" />
  </ResourceDictionary.MergedDictionaries>

  <DataTemplate x:Key="MH.T.IHaveName.Border.RoundDarker" DataType="{x:Type mhuInt:IHaveName}">
    <Border Style="{StaticResource MH.S.Border.RoundDarker}">
      <TextBlock
        Text="{Binding Name}"
        HorizontalAlignment="Center"
        VerticalAlignment="Center"
        Margin="4,0,4,0"/>
    </Border>
  </DataTemplate>
</ResourceDictionary>