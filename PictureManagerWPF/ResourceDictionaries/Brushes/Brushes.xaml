<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

  <Color x:Key="WhiteColor">#FF252525</Color>
  <Color x:Key="BlackColor">White</Color>

  <Color x:Key="GrayColorNormal">#FF7D7D7D</Color>
  <Color x:Key="GrayColor1">#FF333333</Color>
  <Color x:Key="GrayColor5">#FFB9B9B9</Color>
  <Color x:Key="GrayColor7">#FFD8D8D9</Color>
  <Color x:Key="GrayColor9">#5EC9C9C9</Color>

  <Color x:Key="AccentColor">#CC119EDA</Color>
  <Color x:Key="AccentColor2">#99119EDA</Color>
  <Color x:Key="AccentColor3">#66119EDA</Color>
  <Color x:Key="AccentColor4">#33119EDA</Color>

  <SolidColorBrush Color="#FFF9DA77" x:Key="ColorBrushFolder" />
  <SolidColorBrush Color="#FF8EC163" x:Key="ColorBrushTag" />
  <SolidColorBrush Color="#FF137AA6" x:Key="ColorBrushPeople" />
  <SolidColorBrush Color="#FFC7C7C7" x:Key="ColorBrushDrive" />
  <SolidColorBrush Color="#FFFFFFFF" x:Key="ColorBrushWhite" />
  <SolidColorBrush Color="#FF252525" x:Key="ColorBrushBackground" />

  <SolidColorBrush x:Key="TextBrush" Color="{StaticResource BlackColor}" />
  <SolidColorBrush x:Key="WhiteBrush" Color="{StaticResource WhiteColor}" />
  <SolidColorBrush x:Key="BlackBrush" Color="{StaticResource BlackColor}" />

  <SolidColorBrush x:Key="GrayNormalBrush" Color="{StaticResource GrayColorNormal}"/>
  <SolidColorBrush x:Key="GrayBrush1" Color="{StaticResource GrayColor1}"/>
  <SolidColorBrush x:Key="GrayBrush5" Color="{StaticResource GrayColor5}"/>
  <SolidColorBrush x:Key="GrayBrush7" Color="{StaticResource GrayColor7}"/>
  <SolidColorBrush x:Key="GrayBrush9" Color="{StaticResource GrayColor9}"/>

  <SolidColorBrush x:Key="AccentColorBrush" Color="{StaticResource AccentColor}"/>
  <SolidColorBrush x:Key="AccentColorBrush2" Color="{StaticResource AccentColor2}"/>
  <SolidColorBrush x:Key="AccentColorBrush3" Color="{StaticResource AccentColor3}"/>
  <SolidColorBrush x:Key="AccentColorBrush4" Color="{StaticResource AccentColor4}"/>

  <SolidColorBrush x:Key="ActiveSelectionBackgroundBrush" Color="{StaticResource AccentColor}" />
  <SolidColorBrush x:Key="ActiveSelectionForegroundBrush" Color="{StaticResource BlackColor}"/>
  <SolidColorBrush x:Key="DisabledForegroundBrush" Color="{StaticResource GrayColorNormal}"/>
  <SolidColorBrush x:Key="DisabledBackgroundBrush" Color="{StaticResource GrayColor7}"/>
  <SolidColorBrush x:Key="DisabledSelectedBackgroundBrush" Color="{StaticResource GrayColor7}"/>
  <SolidColorBrush x:Key="DisabledSelectedForegroundBrush" Color="{StaticResource WhiteColor}"/>
  <SolidColorBrush x:Key="HoverBackgroundBrush" Color="{StaticResource AccentColor3}"/>
  <SolidColorBrush x:Key="HoverSelectedBackgroundBrush" Color="{StaticResource AccentColor}"/>
  <SolidColorBrush x:Key="SelectedBackgroundBrush" Color="{StaticResource AccentColor2}"/>
  <SolidColorBrush x:Key="SelectedForegroundBrush" Color="{StaticResource BlackColor}"/>

</ResourceDictionary>