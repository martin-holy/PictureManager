<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:pm="clr-namespace:PictureManager"
                    xmlns:converters="clr-namespace:PictureManager.Converters">

  <pm:StaticResourceConverter x:Key="StaticResourceConverter" />
  <pm:IconNameToBrushConverter x:Key="IconNameToBrushConverter" />
  <pm:IconNameToStaticResourceConverter x:Key="IconNameToStaticResourceConverter" />
  <pm:RatingConverter x:Key="RatingConverter" />
  <pm:ImageSourceConverter x:Key="ImageSourceConverter" />
  <pm:MediaItemSizeConverter x:Key="MediaItemSizeConverter" />
  <pm:AllToVisibilityConverter x:Key="AllToVisibilityConverter" />
  <pm:AllToBoolConverter x:Key="AllToBoolConverter" />
  <pm:CatTreeViewMarginConverter x:Key="CatTreeViewMarginConverter" Length="12" />
  <pm:DataTypeConverter x:Key="DataTypeConverter" />
  <converters:CategoryToPathGeometryConverter x:Key="CategoryToPathGeometryConverter" />
  <converters:CategoryToBrushConverter x:Key="CategoryToBrushConverter" />
  <converters:DisplayFilterToBrushConverter x:Key="DisplayFilterToBrushConverter" />
</ResourceDictionary>